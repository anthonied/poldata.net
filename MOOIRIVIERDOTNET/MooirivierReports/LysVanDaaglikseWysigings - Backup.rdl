      <Query>
        <DataSourceName>Mooirivier</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT wysig.polisno, wysig.datum, wysig.beskrywing, wysig_besk.besk, persoonl.versekerde, persoonl.voorl, persoonl.posbestemming
FROM (((poldata5.wysig as wysig LEFT JOIN poldata5.wysig_besk  as wysig_besk ON wysig.kode = wysig_besk.kode) 
 LEFT JOIN poldata5.persoonl as persoonl ON persoonl.polisno = wysig.polisno)
 LEFT JOIN poldata5.area as area ON area.area_kode = persoonl.area)
WHERE left(wysig.datum,10) = @reportDate
AND wysig.kode &lt;&gt; '300'
AND area_besk in (@Area)
ORDER BY persoonl.versekerde,persoonl.voorl, wysig.datum</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@reportDate">
            <Value>=Parameters!reportDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Area">
            <Value>=Parameters!Area.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
